package exit.services.convertidor.jsonAJson;

import org.json.simple.JSONObject;

import exit.services.convertidos.csvAJson.AbstractJsonRestEstructura;
import exit.services.convertidos.csvAJson.JsonGenerico;
import exit.services.singletons.ConfiguracionEntidadParticular;

public class ConvertirJsonAJson {
	
	private ConfiguracionEntidadParticular configEnt;
	private JSONObject formatoJsonAConvertir;
	private JSONObject obtenedorDeDatos;
	
	public ConvertirJsonAJson(ConfiguracionEntidadParticular configEnt, JSONObject formatoJsonAConvertir, JSONObject obtenedorDeDatos) {
		super();
		this.configEnt = configEnt;
		this.formatoJsonAConvertir = formatoJsonAConvertir;
		this.obtenedorDeDatos=obtenedorDeDatos;
	}
	
	public JSONObject convertir(ConfiguracionEntidadParticular confEntidadPart) throws Exception{
		AbstractJsonRestEstructura absJsonRestEst= new JsonGenerico(confEntidadPart);
		System.out.println(absJsonRestEst.getJsonFormato());
		return null;
	}
	
	
	
}
